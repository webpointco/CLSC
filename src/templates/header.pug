nav.navbar(role='navigation', aria-label='main navigation')
  .navbar-brand
    a.navbar-item(href='/index.pug')
      img(src='/images/Bill-CLSC-Logo-Grey.png')
    a.navbar-burger.burger(role='button', aria-label='menu', aria-expanded='false', data-target='navbar')
      span(aria-hidden='true')
      span(aria-hidden='true')
      span(aria-hidden='true')
  .navbar-menu#navbar
    .navbar-end
      .navbar-item
        each link in links
          - let [name, url] = link;
          a.navbar-item(class={'is-active': page === name}, href=url) #{name}
  